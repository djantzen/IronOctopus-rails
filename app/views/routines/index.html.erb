<div>
  <% @routines.each do |routine| %>
    <div>
      <%= link_to(routine.name, user_routine_path(routine.client, routine)) %>
      <% if routine.client == current_user %>
        <%= link_to("Perform", perform_routine_path(routine.client, routine), :class => "btn btn-primary") %>
      <% end %>
    </div>
  <% end %>

  <%= link_to("Create New Routine", new_user_routine_path(current_user), :class => "btn btn-primary") %>
</div>